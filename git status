[32m+[m[32m    <script>[m
[32m+[m[32m        // Prevent flash of light theme by applying dark theme immediately[m
[32m+[m[32m        (function() {[m
[32m+[m[32m            const savedTheme = localStorage.getItem('theme');[m
[32m+[m[32m            if (savedTheme === 'dark') {[m
[32m+[m[32m                document.documentElement.classList.add('dark-theme');[m
[32m+[m[32m                document.documentElement.style.backgroundColor = 'rgba(18, 18, 18, 0.95)';[m
[32m+[m[32m                document.documentElement.style.color = '#e0e0e0';[m
[32m+[m[32m                // Also apply to body when it becomes available[m
[32m+[m[32m                document.addEventListener('DOMContentLoaded', function() {[m
[32m+[m[32m                    document.body.classList.add('dark-theme');[m
[32m+[m[32m                });[m
[32m+[m[32m                // Apply immediately if body already exists[m
[32m+[m[32m                if (document.body) {[m
[32m+[m[32m                    document.body.classList.add('dark-theme');[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        })();[m
[32m+[m[32m    </script>[m
